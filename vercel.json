{
    "version": 2,
    "builds": [
      {
        "src": "app.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "headers": {
          "Content-Security-Policy": "default-src 'self' https://vercel.live; script-src 'self' 'unsafe-inline' https://vercel.live"
        },
        "dest": "app.js"
      }
    ]
  }
  